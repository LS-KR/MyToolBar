<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:sys="clr-namespace:System;assembly=netstandard">

    <sys:String x:Key="ApiLanguage">en</sys:String>
    <sys:String x:Key="WindowTitle">Weather</sys:String>
    <sys:String x:Key="Main_FeelsLike">FeelsLike</sys:String>
    <sys:String x:Key="Main_WindLevel">Level</sys:String>
    <sys:String x:Key="Main_Humidity">Humidity</sys:String>
    <sys:String x:Key="Main_Visibility">Visibility</sys:String>

