<Page x:Class="MyToolBar.Views.Pages.Settings.ComponentsSettingsPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:MyToolBar.Views.Pages.Settings"
      xmlns:ws="https://schemas.elecho.dev/wpfsuite"
      mc:Ignorable="d" Foreground="{DynamicResource ForeColor}"
      d:DesignHeight="450" d:DesignWidth="800"
      Title="ComponentsSettingsPage">
    <ws:ScrollViewer Style="{StaticResource SimpleVerticalScrollViewerStyle}">
        <StackPanel x:Name="PackagesPanel">
            <TextBlock Text="{DynamicResource SettingsWindow_Tab_Components}" FontSize="22" FontWeight="Bold" Margin="20 20 0 0"/>
        </StackPanel>
    </ws:ScrollViewer>
</Page>
